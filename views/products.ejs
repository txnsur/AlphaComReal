    <script>
        document.addEventListener("DOMContentLoaded", function () {
            // Obtiene el parámetro "section" de la URL
            const urlParams = new URLSearchParams(window.location.search);
            const section = urlParams.get("section");
        
            // Mapea las secciones con los IDs de los botones y del contenido adicional
            const sectionsMap = {
                "services-1": { button: "btn-more-services", content: "additional-services" },
                "services-2": { button: "btn-more-solutions", content: "additional-solutions" },
                "services-3": { button: "btn-more-diverse", content: "additional-diverse" },
                "services-4": { button: "btn-more-coverage", content: "additional-coverage" } // Añade más si es necesario
            };
        
            // Verifica si la sección está en el mapa y expande el contenido
            if (section && sectionsMap[section]) {
                const { button, content } = sectionsMap[section];
                const buttonElement = document.getElementById(button);
                const contentElement = document.getElementById(content);
        
                // Muestra el contenido extendido, ajusta el texto del botón y hace scroll hacia la sección
                if (contentElement && buttonElement) {
                    contentElement.style.display = "block";
                    buttonElement.textContent = "Leer menos"; // Cambia el texto a "Leer menos"
        
                    // Desplaza la vista con un margen ajustado para que el contenido quede justo en una posición agradable
                    const offset = 390; // Ajusta este valor según la altura deseada
                    const elementPosition = contentElement.getBoundingClientRect().top + window.scrollY;
                    window.scrollTo({
                        top: elementPosition - offset,
                        behavior: "smooth"
                    });
                }
            }
        });
        </script>
